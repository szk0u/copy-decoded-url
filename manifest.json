{

    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "1.3",
    "homepage_url": "https://github.com/szk0u/copy-decoded-url",
    "default_locale": "ja",

    "background": {
      "scripts": [
        "lib/browser-polyfill.min.js",
        "src/background.js"]
    },

    "permissions": [
      "<all_urls>",
      "activeTab",
      "contextMenus",
      "clipboardWrite",
      "tabs"
    ]

}
